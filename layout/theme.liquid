<!doctype html lang="en">
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <link rel="icon" type="image/png" href="https://cdn.shopify.com/s/files/1/0266/9782/6383/files/favicon.png">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }}
  {{ 'style.css' | asset_url | stylesheet_tag | }}
  
</head>
<body>

{% comment %} {% include 'main-nav' %} {% endcomment %}
  <main role="main">
    {% section  'main-nav' %}
    {{ content_for_layout }}
    {% include 'footer' %}
  </main>
{% comment %} {% include 'footer' %} {% endcomment %}

  {{ 'option_selection.js' | shopify_asset_url | script_tag }}


  <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
  {{ 'app.bundle.js' | asset_url | script_tag }}
</body>
</html>
