    <div class="mm-mega">
      <div class="mm-mega__container">
        <div class="inner">
          <div class="mm-cards">
            <div class="mm-cards-el">
              <h5>Main Menu</h5>
              <nav id="mobile-nav">
                <ul>
                  <li><a href="">Products</a></li>
                  {% assign menuObj = linklists.main-menu %}
                  {% for link in menuObj.links %}
                    <li><a href="{{ link.url }}">{{ link.title }}</a></li>
                  {% endfor %}
                  <li><a href="" id="mobile-search">Search</a></li>
                </ul>
              </nav>
            </div>
  
          </div>
  
          <div class="mm-search">
            <form action="">
              <div class="mm-search__element">
                <input type="text" placeholder="Search">
                <button>Search</button>
              </div>
            </form>
          </div>
  
          <div class="mm-products">
            <h5>Featured Products</h5>
            <div class="mm-slider__group">
              <div class="mm-slider">
                {% for block in section.blocks %}
                  {% assign product_item = all_products[block.settings.featured_product] %}
                    <a class="mm-slider__el" href="{{ product_item.url }}">
                      <div class="mm-slider__el-image">
                          <p>{{ product_item.title }}</p>
                          <div class="section-bg">
                            <div class="section-bg__el lazy-bg" data-bg="{{ product_item | image_url }}">
                            
                            </div>
                          </div>  
                      </div>
                    </a>
                {% endfor %}
              </div>
              <div class="mm-slider-btn align-right push-top">
                <button class="slide-btn prev">
                  <span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="8.414" height="14.828" viewBox="0 0 8.414 14.828">
                      <path d="M2918.577,190.731l-6,6,6,6" transform="translate(2919.991 204.145) rotate(180)" fill="none" stroke="#fff" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"/>
                    </svg>                  
                  </span>
                </button>
                <button class="slide-btn next">
                  <span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="8.414" height="14.828" viewBox="0 0 8.414 14.828">
                      <path d="M2918.577,190.731l-6,6,6,6" transform="translate(2919.991 204.145) rotate(180)" fill="none" stroke="#fff" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"/>
                    </svg>                  
                  </span>
                </button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  
  </div>

  <div class="search dk-only">
    <div class="inner">
      <form action="/search">
        <input type="text" 
        placeholder="Search Products"
        name="q"
        value="{{ search.terms | escape }}"
        >
        <div class="flex-el">
          <div class="cancel">Cancel</div>
          <button class="cta">Search</button>
        </div>
      </form>
    </div>
  </div>
</header>

{% schema %}
{
  "name": "Master Menu",
  "settings": [
    {
      "id": "banner_text",
      "type": "text",
      "label": "Banner Text"
    },
    {
      "id": "banner_link",
      "type": "url",
      "label": "Banner Link"
    }
  ],
  "name": "Featured Products",
  "blocks": [
    {
      "type": "featured_product",
      "name": "Featured Product",
      "settings": [
        {
          "id": "featured_product",
          "type": "product",
          "label": "Product"
        }
      ]
    }
  ]
}
{% endschema %}