{% comment %} <section id="hero">
  <div class="inner">
    <div id="hdr-content" class="article-single is-centered contact-page">
      <h4>We want to hear from you.</h4>
      <h1>{{ page.title }}</h1>
    </div>
  </div>
  <div class="section-bg has-overlay" id="hero_paralax">
  {% assign img = page.metafields.accentuate.hero_image | first %}
  
    <div class="section-bg__el lazy-bg" data-bg="{{img.src}}">
    </div>
  </div>
</section> {% endcomment %}

<div class="header-spacer"></div>
<section>
  <div class="inner push-bottom">
    <div class="inner center intro-content">
      <h3 class="section-subtitle">{{ page.metafields.accentuate.subtitle}} </h3>
      <h1 class="section-title">{{ page.metafields.accentuate.contact_main_title }} </h1>
    </div>
  </div>
</section>



<section>
  <div class="inner re">
  <div class="contact">
    <div class="contact__el">
      
      <div class="contact__logo">
        {% include 'logo-with-text' %}
      </div>
      
      <!--[if lte IE 8]>
<script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2-legacy.js"></script>
<![endif]-->
<script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2.js"></script>
<script>
  hbspt.forms.create({
	portalId: "7850704",
	formId: "60018911-1ec6-42a1-b95a-f26c2142cf80"
});
</script>
    </div>

    <div class="contact__el">
      <h4>Kannaco Contact</h4>
      <div class="contact__copy">
        {{ page.metafields.accentuate.contact_mini_blurb }} 
      </div>
      <ul class="platforms">
        <li>
          <a href="">
            <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" class="feather feather-phone-call"><path d="M15.05 5A5 5 0 0 1 19 8.95M15.05 1A9 9 0 0 1 23 8.94m-1 7.98v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path></svg>
            </span>
            <span>{{ settings.number }}</span>
          </a>
        </li>
        <li>
          <a href="mailto:{{ settings.email }}">
            <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" class="feather feather-mail"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>
            </span>
            <span>{{ settings.email }}</span>
          </a>
        </li>
        <li>
          <a href="{{ settings.instagram_link }}">
            <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" class="feather feather-instagram"><rect x="2" y="2" width="20" height="20" rx="5" ry="5"></rect><path d="M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z"></path><line x1="17.5" y1="6.5" x2="17.51" y2="6.5"></line></svg>
            </span>
            <span>@kannaco.co</span>
          </a>
        </li>
        <li>
          <a href="{{ settings.twitter_link }}">
            <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" class="feather feather-twitter"><path d="M23 3a10.9 10.9 0 0 1-3.14 1.53 4.48 4.48 0 0 0-7.86 3v1A10.66 10.66 0 0 1 3 4s-4 9 5 13a11.64 11.64 0 0 1-7 2c9 5 20 0 20-11.5a4.5 4.5 0 0 0-.08-.83A7.72 7.72 0 0 0 23 3z"></path></svg>
            </span>
            <span>@kannaco.co</span>
          </a>
        </li>
      </ul>
    </div>

    </div>

  {% assign img = page.metafields.accentuate.hero_image | first  %}
    <div class="offset-el">
      <div class="section-bg">
        <div class="section-bg__el lazy-bg" data-bg="{{ img.src }}"></div>
      </div>
    </div>
  
  </div>

  <div class="bg-element">
    {% include 'flower' %}
  </div>
</section>